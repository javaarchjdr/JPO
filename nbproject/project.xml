                <classpath mode="compile">libs/miglayout-4.0.jar:libs/gdata-core-1.0.jar:libs/gdata-maps-2.0.jar:libs/gdata-media-1.0.jar:libs/gdata-photos-2.0.jar:libs/jwizz-0.1.4.jar:libs/commons-net-3.2.jar:libs/javax.mail-1.5.1.jar:libs/commons-compress-1.8.jar:libs/commons-net-3.3.jar:libs/guava-16.0.1.jar:libs/jsch-0.1.51.jar:libs/commons-io-2.4.jar:libs/commons-logging-1.1.3.jar:libs/commons-lang3-3.3.2.jar:libs/TagCloud.jar:libs/commons-jcs-core-2.0-beta-1.jar:libs/commons-jcs-jcache-2.0-beta-1.jar:libs/jxmapviewer2-2.0.jar:libs/xmpcore-5.1.2.jar:libs/metadata-extractor-2.8.1.jar:libs/docking-frames-common.jar:libs/docking-frames-core.jar</classpath>
